<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.huzan.interview.mapper.JobMapper">
    <select id="getJob" resultType="com.huzan.interview.bean.JobBean">
    select * from job
    </select>

    <select id="getJobById" resultType="com.huzan.interview.bean.JobBean">
    select * from job where id=#{id}
    </select>

</mapper>

